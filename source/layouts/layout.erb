<!doctype html>
<html lang="en" class="no-js">
  <head>
    <%= partial 'layouts/google_analytics' %>

    <meta charset="utf-8">

    <title><%= current_page.data.title && "#{current_page.data.title}" %></title>

    <link rel="shortcut icon" href="<%= asset_path :images, 'favicon.ico' %>" type="image/x-icon" />

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="<%= current_page.data.description %>">

    <!--[if gt IE 8]><!--><%= stylesheet_link_tag :screen, media: 'screen' %><!--<![endif]-->
    <!--[if lte IE 8]><%= stylesheet_link_tag 'screen-old-ie', media: 'screen' %><![endif]-->
    <%= stylesheet_link_tag :print, media: 'print' %>

    <!--[if IE 8]><%= stylesheet_link_tag "fonts-ie8", media: :all %><![endif]-->
    <!--[if gte IE 9]><!--><%= stylesheet_link_tag :fonts, media: :all %><!--<![endif]-->

    <%= javascript_include_tag :application %>

  </head>

  <body>
    <%= javascript_include_tag 'javascripts/check_cookie' %>

    <%= partial 'shared/cookie_policy_banner' %>
    <%= partial 'layouts/google_analytics_noscript' %>

    <a href="#main" class="skip-link">Skip to main content</a>

    <header class="govuk-header" role="banner" data-module="govuk-header">
      <div class="govuk-header__container govuk-width-container">
        <div class="govuk-header__logo">
          <a href="/" class="govuk-header__link govuk-header__link--homepage" data-click-events data-click-category="Header" data-click-action="Logo link clicked">
            <span class="govuk-header__logotype">
              <svg role="presentation" focusable="false" class="govuk-header__logotype-crown" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 132 97" height="30" width="36">
